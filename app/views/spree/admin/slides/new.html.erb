<% render partial: 'page_action', locals: {slide: @slide} %>

<% content_for :page_title do %>
  <%= Spree.t('slide.new_slide') %>
<% end %>


<%= render "spree/shared/error_messages", target: @slide %>

<%= form_for [:admin, @slide], html: {multipart: true} do |f| %>
  <%= render partial: 'form', locals: { f: f, slide: @slide } %>
  <%= render partial: 'spree/admin/shared/edit_resource_links' %>
<% end %>