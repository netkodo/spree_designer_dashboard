<%= form_for(@designer_registration) do |f| %>
  <div class="form-group">
    <div class="row">
      <div class="col-lg-12">
        <%= f.label :tin, "TIN/SSN" %>
        <%= f.text_field :tin, :class => 'form-control' %>
        <%= f.error_message_on :tin %>
      </div>
    </div>
  </div>
  <% Spree::PaymentMethod.where(active: true, environment: Rails.env, display_on: ['frontend','']).each do |method| %>
    <fieldset class="text-center">
      <%= render :partial => "spree/checkout/payment/#{method.method_type}", :locals => {:payment_method => method} %>
    </fieldset>
  <% end %>
  <div class="actions">
    <%= f.submit "Upgrade", :class => "btn btn-primary" %>
  </div>
<% end %>