<div class="row">
    <div class="col-md-3">
        <%= render :partial => "spree/boards/dashboard_menu", :locals => {:selected => "dashboard"} %>
    </div>

    <div class="col-md-9">
      <div class="col-md-12 text-center">
        <h1>Edit Project Details</h1>
      </div>

        <%= form_for @project,:url => project_path(@project), html: {id: "create_project_form"}, method: :put do |f| %>
          <div class="row">
             <div class="col-md-8 col-md-offset-2">

              <%= render :partial => "form", locals: {f: f} %>

             </div>
          </div>

          <div class="row">
              <div class="form-actions text-right col-md-8 col-md-offset-2">
                  <%= f.submit "Update", :class => 'btn btn-primary' %>
              </div>
          </div>
        <% end %>
    </div>
</div>